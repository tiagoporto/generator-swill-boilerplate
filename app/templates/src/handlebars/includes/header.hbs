<!DOCTYPE html>
<html lang="{{#if lang}}{{lang}}{{else}}en{{/if}}">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>{{#if title}}{{title}}{{else}}{-}{{/if}}</title>
        <meta name="description" content="{{#if description}}{{description}}{{else}}{-}{{/if}}">
        <meta name="keywords" content="{{#if keywords}}{{keywords}}{{else}}{-}{{/if}}">
        <meta name="viewport" content="width=device-width, initial-scale=1"><% if (include.humans) { %>
        <link rel="author" href="humans.txt" /><% } %><% if (use.jqueryLogoDownloadtip) { %>

        <!-- Logos downloadtip -->
        <link rel="icon" type="image/png" href="img/logos/logo.png" data-title="Screen Bitmap file (.png)" />
        <link rel="icon" type="image/png" href="img/logos/logo-monochrome.png" data-title="Screen Monochrome Bitmap file (.png)" />
        <link rel="icon" type="image/svg+xml" href="img/logos/logo.svg" data-title="Screen Vector file (.svg)" />
        <link rel="icon" type="image/svg+xml" href="img/logos/logo-monochrome.svg" data-title="Screen Monochrome Vector file (.svg)" />
        <link rel="icon" type="image/eps" href="img/logos/logo.eps" data-title="Print file (.eps)" />
        <link rel="icon" type="image/eps" href="img/logos/logo-monochrome.eps" data-title="Print Monochrome file (.eps)" /><% } %>

        <!-- Add to homescreen for Chrome on Android -->
        <meta name="theme-color" content="{-}">
        <meta name="mobile-web-app-capable" content="yes">
        <link rel="icon" sizes="192x192" href="img/touch/chrome-touch-icon-192x192.png">

        <!-- Add to homescreen for Safari on iOS -->
        <meta name="apple-mobile-web-app-capable" content="yes">
        <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
        <meta name="apple-mobile-web-app-title" content="{{title}}">
        <link rel="apple-touch-icon" href="apple-touch-icon.png">

        <!-- Tile icon for Win8 -->
        <meta name="application-name" content="{{title}}"/>
        <meta name="msapplication-TileColor" content="{-}"/>
        <meta name="msapplication-square70x70logo" content="img/touch/tile.png"/>
        <meta name="msapplication-square150x150logo" content="img/touch/tile.png"/>
        <meta name="msapplication-wide310x150logo" content="img/touch/tile-wide.png"/>
        <meta name="msapplication-square310x310logo" content="img/touch/tile.png"/>

        <!-- SEO: If your mobile URL is different from the desktop URL, add a canonical link to the desktop page https://developers.google.com/webmasters/smartphone-sites/feature-phones -->
        <!--
        <link rel="canonical" href="http://www.example.com/">
        -->

        <!-- build:css css/styles.combined.min.css --><% if (use.normalize) { %>
        <link rel="stylesheet" href="../node_modules/normalize.css/normalize.css"><% } %><% if (use.outdatedBrowser) { %>
        <link rel="stylesheet" href="../node_modules/outdated-browser/outdatedbrowser/outdatedbrowser.css"><% } %><% if (use.jqueryLogoDownloadtip) { %>
        <link rel="stylesheet" href="../node_modules/jquery-logo-downloadtip/css/jquery-logo-downloadtip.css"><% } %>
        <link rel="stylesheet" href="css/styles.css">
        <!-- endbuild -->
    </head>
    <body><% if (use.outdatedBrowser) { %>
        <!-- ============= Outdated Browser ============= -->
        <div id="outdated"></div>
        <!-- ============================================ --><% } %>

    {{> @partial-block }}

{{> footer}}
